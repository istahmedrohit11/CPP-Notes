#include<iostream>
using namespace std;
int main(){
    int age;
    cout<<"Tell me your age: ";
    cin>>age;
    switch (age)
    {
    case 18:
        cout<<"You are 18";
        break;
    case 20:
        cout<<"You are 22";
        break;
    default:
        cout<<"No special cases";
        break;
    }

}
