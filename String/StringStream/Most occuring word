#include<bits/stdc++.h>
#include<sstream>
using namespace std;

int main(){
    string str="Rohit is a a  e e CSE student";
    stringstream ss(str);
    string temp; 
    vector<string> v;
    while(ss>>temp){
        v.push_back(temp);
    }
    sort(v.begin(),v.end());
    int maxcount=1;
    int count=1;
     for(int i=0;i<v.size()-1;i++){
        if(v[i]==v[1+i]) count++;
        else count=1;
        maxcount=max(maxcount,count);
    }
    //cout<<maxcount;
    //int count =1;
     for(int i=0;i<v.size()-1;i++){
        if(v[i]==v[1+i]) count++;
        else count=1;
        if(count==maxcount){
            cout<<v[i]<<" "<<maxcount<<endl;
        }
    }
}
