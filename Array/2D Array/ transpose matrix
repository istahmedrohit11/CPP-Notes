#include<bits/stdc++.h>
using namespace std;   

int main(){
    int arr[2][4]={{12,1,14,155},
                    {21,22,23,25}}; 
   
//     for(int i=0;i<2;i++){
//         for(int j=0;j<4;j++){
//             cout<<arr[i][j]<<" ";
//         }
//         cout<<endl;
//     }
    //transpose matrix
    for(int i=0;i<4;i++){
        for(int j=0;j<2;j++){
            cout<<arr[j][i]<<" ";
        }
        cout<<endl;
    }
} 
