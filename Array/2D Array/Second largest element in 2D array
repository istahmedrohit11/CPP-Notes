#include<bits/stdc++.h>
using namespace std; 

int main(){
    int arr[2][4]={{76,88,82,13},
                   {80,84,99,83}};
        int max=arr[0][0];
        int max2=arr[0][0];
        for(int i=0;i<2;i++){
            for(int j=0;j<4;j++){
                if(max<arr[i][j]){
                    max2 = max;
                    max=arr[i][j];
                }else if(arr[i][j]>max2 && arr[i][j]<max){
                    max2=arr[i][j];
                }
            }
        }

        cout << "Largest: " << max << endl;
        cout << "Second Largest: " << max2 << endl;
}  
