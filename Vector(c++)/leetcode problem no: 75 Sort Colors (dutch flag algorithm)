#include<bits/stdc++.h>
using namespace std;   
void  sortNegPos(vector<int> &nums){
    int n=nums.size();
    int low=0 ,mid=0 , high=n-1;
    while(mid<=high){
        if(nums[mid]==2){
            swap(mid,high);
            high--;
        }
        if(nums[mid]==0){
            swap(mid,low);
            low++;
            mid++;
        }
        if(nums[mid]==1){
            mid++;
        }
    }
}
int main(){
    vector<int>v1={0,1,2,1,0,1,2};   
    sortNegPos(v1);
    for(int i=0;i<v1.size();i++){
        cout<<v1[i]<<" "; 
    }
}  
