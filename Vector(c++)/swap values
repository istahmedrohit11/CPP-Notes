#include<bits/stdc++.h>
using namespace std;   

int main(){
    vector<int>v1={1,3,2,4,3,4,1,6};   
    vector<int>v2(v1.size());
    int i=0,j=v1.size()-1;
    while(i<j){
        swap(v1[i],v1[j]);
        i++;
        j--;
    }
    for(int i=0;i<v1.size();i++){
        cout<<v1[i]<<" ";
    }
} 
