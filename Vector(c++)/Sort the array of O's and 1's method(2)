//two pointers method(two variable)

#include<bits/stdc++.h>
using namespace std;   
void sort10(vector<int> &v){
    int n=v.size();
     int i=0;
     int j=n-1;
     while(i<j){
        if(v[j]==1) j--; 
        else if(v[i]==0) i++;
       
        else{  //v[i]==1 && v[j]==0
            swap(v[i],v[j]);
            i++;
            j--;
        }
     }
}
int main(){
    vector<int>v1={0,1,0,0,1,1,0,1};   
    sort10(v1);
    for(int i=0;i<v1.size();i++){
        cout<<v1[i]<<" "; 
    }
} 
