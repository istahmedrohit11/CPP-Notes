Two Pass Metnod

#include<bits/stdc++.h>
using namespace std;   
void sort10(vector<int> &v){
    int n=v.size();
    int nOne=0;
    int nZero=0;
    for(int i=0;i<n;i++){
        if(v[i]==0) nZero++;
        else nOne++;
    }
    //filling elements
    for(int i=0;i<n;i++){
        if(i<nZero) v[i]=0;
        else v[i]=1;  
    }
}
int main(){
    vector<int>v1={0,1,0,0,1,1,0,1};   
    sort10(v1);
    for(int i=0;i<v1.size();i++){
        cout<<v1[i]<<" "; 
    }
} 
