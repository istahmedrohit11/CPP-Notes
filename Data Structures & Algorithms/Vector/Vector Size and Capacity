#include<iostream>
#include<vector>
using namespace std;

int main(){
    vector<char>number={1,2};
    cout<<"Size:"<<number.size()<<endl;
    cout<<"Capacity:"<<number.capacity()<<endl;

    //capacity size is increasing like 2 to the power 2,3,4 like this.
    number.push_back(3);
    cout<<"Capacity:"<<number.capacity()<<endl;
    number.push_back(4);
     cout<<"Capacity:"<<number.capacity()<<endl;
    number.push_back(5);
    cout<<"Capacity:"<<number.capacity()<<endl;
    number.push_back(6);

    cout<<"Size:"<<number.size()<<endl;
    cout<<"Capacity:"<<number.capacity()<<endl;
}
