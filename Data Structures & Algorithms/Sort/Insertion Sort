#include<iostream>
using namespace std;
int main(){
    int arr[]={5,3,4,1,2,-8,10};
    int n=sizeof(arr)/sizeof(arr[0]);
    // Insertion Sort
    for(int i=1;i<n;i++){ 
        int j=i;
        while(j>0){
            if(arr[j]>=arr[j-1]) break;
            else{
                swap(arr[j],arr[j-1]);  
            }
            j--;
        }
    }
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
    
    
}
