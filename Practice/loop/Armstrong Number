#include<iostream>
using namespace std;
int main(){
    int x,y;
    cout<<"Enter Start position:";
    cin>>x;
    cout<<"Enter End position:";
    cin>>y;
    int digit;
    for(int i=x;i<=y;i++){
        int temp=i,sum=0;
        if(temp<=9){
            cout<<i<<" is Armstrong Number"<<endl;
        }
        while(temp!=0){
        digit=temp%10;
        sum=sum+ digit*digit*digit;
        temp=temp/10;
    }
     if(i==sum){
            cout<<i<<" is Armstrong Number"<<endl;
        }
    } 
}
