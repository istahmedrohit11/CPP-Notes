#include<iostream>

using namespace std;
int main(){
    char gender ='F';
    bool ismarried = true;
    unsigned short sons=2;// [0, 255]
    short year=2000; // [-32767, 32768]
    unsigned int package=1500000;// [0, 4294967295]
    double height=79.48;
    float gpa=4.55F;
    long salary=122346674;
    long long balance = 123212434323443333;

    cout<<"The Gender is : "<<gender <<endl;
    cout<<"Is she married? : "<< ismarried<<endl;
    cout<<"Number of sons she has : "<<sons <<endl;
    cout<<"Year of her appointment : "<< year<<endl;
    cout<<"Salary for a year :"<< package<<endl;
    cout<<"Height is : "<< height<<endl;
    cout<<"GPA is"<<gpa <<endl;
    cout<<"Salary drawn upto : "<<salary <<endl;
    cout<<"Balance till :"<< balance<<endl;
    
    
    
    
    
    
     
    
    
   
}
