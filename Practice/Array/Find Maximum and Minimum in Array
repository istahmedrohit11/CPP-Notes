#include<iostream>
using namespace std;

int main(){
    int arr[5]={2,33,44,55,101};
    int max=arr[0];
    int length= sizeof(arr) / sizeof(arr[0]);
    for(int i=1;i<length;i++){
        if(arr[i]>max){
            max=arr[i];
        }
    }
    cout<<"Maximum number is:"<<max<<endl;
    int min=arr[0];
    for(int i=1;i<length;i++){
        if(arr[i]<min){
            min=arr[i];
        }
    }
    cout<<"Minimum number is:"<<min<<endl;
}
