#include<bits/stdc++.h>
using namespace std;   
void display(int arr[],int size){
    for(int i=0;i<=size-1;i++){
        cout<<arr[i]<<" ";
    }
}
void change(int b[],int size){
    b[0]=100;
    b[4]=99;
}
 
int main(){
    int arr[]={1,4,2,7,8};
    int size=sizeof(arr)/sizeof(arr[0]);
    display(arr,size);
    cout<<endl;
    change(arr,size);
    cout<<endl;
    display(arr,size);
}  
