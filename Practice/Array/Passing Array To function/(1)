#include<bits/stdc++.h>
using namespace std;   
void display(int arr[]){
    for(int i=0;i<=4;i++){
        cout<<arr[i]<<" ";
    }
}
void change(int b[]){
    b[0]=100;
    b[4]=99;
}

int main(){
    int arr[]={1,4,2,7,8};
    display(arr);
    cout<<endl;
    change(arr);
    cout<<endl;
    display(arr);
}  
