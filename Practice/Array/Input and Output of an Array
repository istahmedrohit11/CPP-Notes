#include<iostream>
using namespace std;
 
int main(){
    int n;
    cout<<"Enter the Size of the Array:";
    cin>>n;
    cout<<"Enter "<<n<<" integers:";
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    //cout<<"Array Elements are:";
    for(int i=0;i<n;i++){
        cout<<"Array element index at "<<i<<" is "<<arr[i]<<endl;
        // if(i<n-1){
        //     cout<<","<<endl;
        // }
    }
}
