#include<iostream>
using namespace std;

int main(){
    int arr[]={1,3,5,6,8};
    int length=sizeof(arr)/sizeof(arr[0]);
    bool sorted=true;
    for(int i=0;i<length-1;i++){
        if(arr[i]>arr[i+1]){
            sorted=false;
            break;
        }
    }
    if(sorted){
        cout<<"Sorted";
    }else{
        cout<<"Not Sorted";
    }
}

    
    

