#include<iostream>
using namespace std;
int main(){
    float d=34.4f;
    long double e= 34.4l;
    cout<<"d= "<<d<<endl<<"e= "<<e<<endl;
    cout<<"The size of d is: "<<sizeof(34.4)<<endl;
    //by default
    cout<<"The size of d is: "<<sizeof(34.4f)<<endl;
    cout<<"The size of d is: "<<sizeof(34.4l)<<endl;

    
}
